@linei:@template"a1@import "compass_twitter_bootstrap";

body {
	background-color: #E5E5E5;
	font-size: 14px;
/* 	font-family: 'Open Sans'; */
	font-family: 'Roboto';

	[class^="icon-"],
	[class*=" icon-"] {
		background-image: url('http://getbootstrap.com/2.3.2/assets/img/glyphicons-halflings.png');
	}
}

#black-bar {
	width: 100%;
	height: 30px;
	background-color: #2D2D2D;
	padding: 0px;
	margin: 0px;

	li {
		list-style-type: none;
		float: left;
		margin-left: 24px;
		margin-top: 5px;

		&:first-child {
			margin-left: 38px;
			color: white;
		}

		a {
			color: #B0B0B0;
			text-decoration: none;

			&:hover {
				color: white;
			}
		}
	}
}

.row {
	background-color: white;
}

#search-container {
	height: 72px;
	width: 100%;
	background-color: white;
	background-image: url('../images/search-bar.png');
	background-repeat: no-repeat;
	padding: 0px 0px 0px 0px;
	margin: 10px 0px 0px 40px;
}

#grey-bar {
	height: 50px;
	width: 100%;
	background-color: #F5F5F5;
	padding: 0px 0px 0px 0px;
	margin: 0px 0px 0px 0px;

	#filters {
		ul {
			li {
				list-style-type: none;
				float: left;
				margin-left: 24px;
				margin-top: 5px;

				&:first-child {
					margin-left: 600px;
				}

				a {
					color: #3B3B3B;
					text-decoration: none;
					padding-bottom: 16px;
				}

				.active {
					border-bottom-color: #427FED;
					border-bottom-width: 2px;
					border-bottom-style: solid;
				}

				&:hover {
					text-decoration: none;
					border-bottom-color: #427FED;
					border-bottom-width: 2px;
					border-bottom-style: solid;
					padding-bottom: 16px;
				}
			}
		}
	}
}

.main-header {
	padding: 5px;
	text-align: center;
	color: black;
	text-shadow: #222 0px -1px 1px;
	position: fixed;
	width: 100%;
	left: 0;
	transition: all 0.3s ease;
	
	a {
		position: absolute;
		left: 20px;
		top: 20px;
		color: black;
		font-size: 32px;
		text-decoration: none;
		margin-top: -55px;
	}
}

.page-wrap {
	float: right;
	width: 100%;
	transition: width 0.3s ease;
}

.content {
	padding: 20px 20px 20px 20px;
}

.close-menu {
	display: none;
}

#main-nav:target {
	width: 20%;
}

#main-nav:target + .page-wrap {
	width: 80%;
	
	.open-menu {
		display: none; 
	}
	
	.close-menu {
		display: block;
	}
}

#container {
    width: 1200px;
    margin: 0 auto;
	
	#myContent {
		-moz-column-count: 3;
		-moz-column-gap: 10px;
		-webkit-column-count: 3;
		-webkit-column-gap: 10px;
		column-count: 3;
		column-gap: 10px;
		width: 1200px;
		height: 784px;
		
		li {
			background-color: white;
			padding: 10px;
			margin-top: 10px;
			margin-bottom: 10px;
			background-clip: padding-box;
			border: solid transparent;
			border-width: 1px 1px 1px 6px;
			box-shadow: inset 1px 1px 0 rgba(0,0,0,.1),inset 0 -1px 0 rgba(0,0,0,.07);
			
			&:first-child {
				margin-top: 0px;
			}
			
			h1 {
				color: grey;
				font-size: 22px;
				line-height: 22px;
			}
		}
	}
	
	#myContent img{
		display: block;
		margin-bottom: 0px;
		margin-right: auto;
		margin-left: auto;
	}
}


